# InsightAnalyzerAI 環境変数設定
# このファイルを .env にコピーして使用してください

# 環境設定
INSIGHT_ENV=development  # development / production

# LLM設定（Phase 2）
OPENAI_API_KEY=sk-your-api-key-here
OPENAI_MODEL=gpt-4o-mini  # gpt-4o / gpt-4o-mini / gpt-3.5-turbo

# データ制限
MAX_FILE_SIZE_MB=50
MAX_QUERY_ROWS=100000

# 出力設定
OUTPUT_DIR=output
CHART_DPI=100

# ログ設定
LOG_LEVEL=INFO  # DEBUG / INFO / WARNING / ERROR
